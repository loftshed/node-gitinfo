{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "new-cap": [2, { "capIsNewExceptions": ["Router"] }],
    "arrow-parens": [2, "always"],
    "prefer-const": [
      2,
      {
        "destructuring": "all",
        "ignoreReadBeforeAssign": false
      }
    ],
    "no-var": 2,
    "prefer-template": 2,
    "template-curly-spacing": [2, "never"],
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "eqeqeq": [2, "always"],
    "no-use-before-define": [2, { "functions": false, "classes": true }],
    "no-unused-vars": [2, { "args": "after-used", "ignoreRestSiblings": true }],
    "no-implicit-globals": 2,
    "no-return-await": 2,
    "no-shadow": [2, { "builtinGlobals": false, "hoist": "functions", "allow": [] }],
    "no-trailing-spaces": 2,
    "object-curly-spacing": [2, "always"],
    "semi": [2, "always"],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  }
}
